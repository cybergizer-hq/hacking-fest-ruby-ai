<div id="<%= dom_id comment %>">
  <p>
    <strong>User:</strong>
    <%= comment.user_id %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= comment.message %>
  </p>

  <p>
    <strong>Label:</strong>
    <%= comment.label %>
  </p>

  <p>
    <strong>Emoji:</strong>
    <%= comment.emoji %>
  </p>

  <p>
    <strong>Score:</strong>
    <%= comment.score %>
  </p>

</div>
